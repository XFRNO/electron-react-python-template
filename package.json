{
   "name": "electron-react-python-template",
   "private": true,
   "version": "1.0.0",
   "scripts": {
      "build": "turbo run build",
      "dev": "turbo run dev --filter=electron",
      "postinstall": "pnpm --filter backend exec uv sync",
      "lint": "turbo run lint",
      "format": "prettier --write \"**/*.{ts,tsx,md}\"",
      "check-types": "turbo run check-types",
      "clean": "turbo run clean && rm -rf node_modules .turbo && pnpm store prune"
   },
   "devDependencies": {
      "@types/node": "^24.10.0",
      "@types/wait-on": "^5.3.4",
      "prettier": "^3.6.2",
      "rimraf": "^6.1.0",
      "turbo": "^2.5.8",
      "typescript": "5.9.2"
   },
   "engines": {
      "node": ">=18"
   },
   "packageManager": "pnpm@10.19.0",
   "dependencies": {
      "dotenv": "^17.2.3",
      "get-port": "^7.1.0"
   }
}
